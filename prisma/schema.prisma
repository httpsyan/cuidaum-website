generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id        String   @id @default(cuid())
  name      String
  email     String  
  phone     String  
  profile   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}